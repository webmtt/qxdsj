<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nmpiesat.idata.industry.dao.IndustryApplicationDao">
    <select id="findList" resultType="com.nmpiesat.idata.industry.entity.IndustryApplication">
       select * from industry_application
    </select>

    <select id="findExampleById" resultType="com.nmpiesat.idata.industry.entity.IndustryApplication">
       select * from industry_application where id=#{id}
    </select>
</mapper>